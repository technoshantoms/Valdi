{
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "baseUrl": ".",
        "lib": [
            "dom",
            "ES2019"
        ],
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "importHelpers": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "rootDirs": [
            ".",
        ],
        "typeRoots": [
            "."
        ]
    }
}
